<ruleset comparisonMethod="maven"
         xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 https://www.mojohaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
    <rules>
        <rule groupId="org.apache.tomcat" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">11.*</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.apache.groovy" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">5.*</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.hibernate.orm" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">7.*</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.hibernate.validator" comparisonMethod="maven">
            <ignoreVersions>
                <ignoreVersion type="regex">9.*</ignoreVersion>
            </ignoreVersions>
        </rule>
    </rules>
</ruleset>
